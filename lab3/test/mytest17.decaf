class A {
  int x;
  void p(int a) {
    Print("HEY!", a, "\n");
    x = a;
  }
  void q() {
    Print("BLAH!", x, "\n");
  }
}

class main
{
  void main() {
  A[] b;
  int i;
  b = NewArray(100, A);
  for(i = 0; i < 100; i++) {
    b[i] = new A;
    b[i].p(i);
  }

  for(i = 0; i < 100; i++) {
    b[i].q();
  }
}
}

